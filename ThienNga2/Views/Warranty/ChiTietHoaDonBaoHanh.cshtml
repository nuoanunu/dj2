                                                                    <option value="@user.Id"> @user.FullName</option>
                                                                }
                                                            </select>
                                                            <button type="submit"> Giao </button>
                                                        </form>
                                                    </div>
                                                    <div class="modal-footer">
                                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        }

