                                                    <th>
                                                        Ngày bắt đầu
                                                    </th>
                                                    <th>
                                                        Ngày trả
                                                    </th>
                                                    <th>
                                                        Người sữa
                                                    </th>
                                                    <th>
                                                        Linh kiện
                                                    </th>
                                                    <th>
                                                        Sữa chữa khác
                                                    </th>
                                                    <th>
                                                        Tình trạng lúc nhận
                                                    </th>
                                                    <th>
                                                        Chi tiết
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @if (act.tb_warranty != null)
                                                {


                                                    foreach (tb_warranty_activities oldact in act.tb_warranty.tb_warranty_activities.Where(u => u.status == 6 || u.status == 7).ToList())
                                                    {
                                                        <tr>
                                                            <td>
                                                                @oldact.startDate
                                                            </td>
                                                            <td>
                                                                @oldact.realeaseDATE
                                                            </td>
                                                            @if (oldact.AspNetUser1 != null)
                                                            {
                                                                <td>@oldact.AspNetUser1.FullName</td>
                                                            }
                                                            else
                                                            {
                                                                <td></td>
                                                            }
                                                            <td>
                                                                @foreach (warrantyActivityFee fee in oldact.warrantyActivityFees)
                                                                {<ul>
                                                                    <li>@fee.productSKU : @fee.quantity cái</li>
                                                                </ul>

                                                                }
                                                            </td>
                                                            <td>
                                                                @foreach (warrantyActivityFixingFee fee in oldact.warrantyActivityFixingFees)
                                                                {
                                                                    <ul>
                                                                        <li>@fee.FixDetail</li>
                                                                    </ul>
                                                                }
                                                            </td>
                                                            <td>
                                                                @oldact.Description
                                                            </td>
                                                            <td>
                                                                <a href="/Warranty/ActivityDetail?id=@oldact.id">Detail</a>
                                                            </td>
                                                        </tr>
                                                    }
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>


                            </div>

                        </div>
                        <div class="col-md-3 col-sm-3 col-xs-12">

