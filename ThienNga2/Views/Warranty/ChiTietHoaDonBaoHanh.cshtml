                                    <div class="clearfix"></div>
                                </div>
                                <div class="panel-body">
                                    @if (act.tb_warranty != null)
                                    { <h3 class="green"><i class="fa fa-paint-brush"></i>@act.tb_warranty.item.tb_product_detail.productStoreID</h3>

                                        if (act.tb_warranty.item.tb_customer.CustomerType != null)
                                        {
                                            <div class="project_detail" style="background-color:@act.tb_warranty.item.tb_customer.CustomerType.Color">

                                                <p class="title">Nhóm khách</p>
                                                <p>@act.tb_warranty.item.tb_customer.CustomerType.GroupName</p>

